{
  "name": "webpack-react-spa",
  "version": "3.0.0",
  "private": true,
  "description": "Webpack for ant design.",
  "main": "index.js",
  "scripts": {
    "upgrade": "./bin/upgrade.rb",
    "dll": "cross-env NODE_ENV=dll webpack -p",
    "pretty": "prettier --write src/**/*.{js,scss}",
    "start": "npm run dll && cross-env NODE_ENV=local webpack-dev-server --watch-poll",
    "build:beta": "npm run dll && cross-env NODE_ENV=beta NODE_OPTIONS=--max_old_space_size=4096 webpack -p",
    "build:production": "npm run dll && cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 webpack -p",
    "serve:dist": "node ./build/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afeiship/webpack-react-spa.git"
  },
  "keywords": [
    "webpack",
    "react",
    "pc",
    "ant-design",
    "site",
    "website"
  ],
  "author": "afei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/afeiship/webpack-react-spa/issues"
  },
  "homepage": "https://github.com/afeiship/webpack-react-spa#readme",
  "dependencies": {
    "@feizheng/bomb-decorator": "^1.0.1",
    "@feizheng/http-rest-config": "^2.5.1",
    "@feizheng/mixin-decorator": "^2.0.2",
    "@feizheng/next-axios": "^3.0.0",
    "@feizheng/next-global": "^1.0.2",
    "@feizheng/next-js-core2": "^2.6.0",
    "@feizheng/next-offline-sw": "^1.0.2",
    "@feizheng/next-react-redux": "^3.0.2",
    "@feizheng/next-store": "^3.1.2",
    "@feizheng/next-values": "^1.0.2",
    "@feizheng/react-condition-manager": "^1.1.2",
    "@feizheng/react-full-image": "^1.0.0",
    "@feizheng/react-if-else": "^1.2.0",
    "@feizheng/react-layout-trbla": "^1.0.0",
    "@feizheng/react-mixin-on-change": "^1.0.0",
    "@feizheng/render-null-decorator": "^1.0.0",
    "@feizheng/service-decorator": "^1.0.1",
    "@feizheng/webkit-sass-functions": "^1.0.0",
    "@feizheng/webkit-sass-generators": "^1.2.1",
    "@feizheng/webkit-sass-mixins": "^1.0.0",
    "@feizheng/webkit-sass-reset": "^1.0.0",
    "@feizheng/webkit-sassui-short-name": "^1.2.2",
    "@feizheng/webkit-sassui-transform-center": "^1.0.0",
    "antd": "^4.6.6",
    "antd-form-builder": "^2.1.1",
    "classnames": "^2.2.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-imported-component": "^6.3.13",
    "react-loadable": "^5.5.0",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@feizheng/next-flatten": "^1.1.0",
    "@feizheng/rake-git": "^1.2.1",
    "@feizheng/webpack-app-kits": "^3.4.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-unused-imports": "^0.1.3",
    "husky": "^4.3.0",
    "lint-staged": ">=10",
    "postcss-modules-resolve-from-alias": "^1.3.0",
    "prettier": "^2.1.2",
    "standard": "^14.3.4",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  }
}
